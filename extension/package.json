{
  "name": "cx-master",
  "version": "1.0.1",
  "displayName": "Code-Expert Master",
  "author": "micha4w",
  "description": "A helper Addon to improve the Code-Expert experience",
  "scripts": {
    "build": "vite build",
    "watch": "vite build --watch --mode development --minify false",
    "package": "npm run clean && npm run build && web-ext build -s dist -a out --overwrite-dest",
    "clean": "rm -rf dist",
    "dev": "vite",
    "serve:firefox": "web-ext run -s dist -u 'https://expert.ethz.ch/ide2/XQqxKCymuopJ8SuNG?fileKey=bRyeDpsqsP5Pmb7K6' -p '/home/micha4w/.mozilla/firefox/m41vdnzq.micha4w'",
    "serve:chrome": "web-ext run -t chromium --start-url \"https://example.com\" --source-dir ./dist/",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "license": "MIT",
  "devDependencies": {
    "@samrum/vite-plugin-web-extension": "^5.0.0",
    "@sveltejs/vite-plugin-svelte": "^2.1.1",
    "@tsconfig/svelte": "^4.0.1",
    "@types/ace": "^0.0.51",
    "@types/chrome": "^0.0.228",
    "@types/webextension-polyfill": "^0.10.0",
    "ace-builds": "^1.22.0",
    "svelte": "^3.58.0",
    "svelte-check": "^3.2.0",
    "svelte-preprocess": "^5.0.3",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "vite": "~4.3.3",
    "web-ext": "^7.6.2",
    "xterm": "^5.3.0",
    "xterm-link-provider": "^1.3.1"
  },
  "dependencies": {
    "webextension-polyfill": "^0.10.0"
  },
  "type": "module"
}
